{
  "name": "iter8-cli",
  "version": "1.0.0",
  "description": "AI-powered coding assistant CLI with RAG capabilities",
  "main": "dist/cli.js",
  "type": "module",
  "scripts": {
    "start": "node dist/cli.js",
    "dev": "tsx src/cli.tsx",
    "build": "tsc",
    "build:binary": "pkg dist/cli.js --target node18-macos-arm64 --output iter8-macos",
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "clean": "rm -rf dist .iter8_backups"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.1",
    "@supabase/supabase-js": "^2.39.0",
    "openai": "^4.20.1",
    "ink": "^4.4.1",
    "ink-text-input": "^5.0.1",
    "react": "^18.2.0",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "dotenv": "^16.3.1",
    "figlet": "^1.7.0",
    "glob": "^10.3.10",
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/fs-extra": "^11.0.4",
    "@types/figlet": "^1.5.8",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "keywords": [
    "ai",
    "coding-assistant",
    "cli",
    "rag",
    "huggingface",
    "typescript",
    "code-generation"
  ],
  "author": "Iterate-8",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iterate-8/iter8-cli.git"
  },
  "bugs": {
    "url": "https://github.com/iterate-8/iter8-cli/issues"
  },
  "homepage": "https://github.com/iterate-8/iter8-cli#readme"
}